    status = "okay";
};

&vpu_mmu {
    status = "okay";
};

/* UART -------------------------------------------------------------- */
&uart0 {
    pinctrl-names = "default";
    pinctrl-0 = <&uart0_xfer &uart0_rts &uart0_cts>;
    status = "okay";
};

&uart1 {
    pinctrl-names = "default";
    pinctrl-0 = <&uart1_xfer>;
    status = "okay";

    gnss {
        compatible = "u-blox,neo-m8";
        current-speed = <9600>;
        v-bckp-supply = <&vcc3v3_s5>;
        vcc-supply = <&vcc3v3_sys>;
    };
};

&uart2 {
    status = "okay";
};

/* SDIO -------------------------------------------------------------- */
&sdio0 {
    status = "okay";

    #address-cells = <1>;
    #size-cells = <0>;

    mmc-pwrseq = <&wifi_sdio_pwrseq>;

    pinctrl-names = "default";
    pinctrl-0 = <&sdio0_bus4 &sdio0_cmd &sdio0_clk>;

    bus-width = <4>;
    cap-sdio-irq;
    cap-sd-highspeed;
    cap-power-off-card;
    keep-power-in-suspend;
    non-removable;
    broken-cd;
    no-sd;
    no-mmc;
    disable-wp;

    clock-frequency = <52000000>;

    wlcore: wlcore@0 {
        compatible = "ti,wl1831";
        reg = <2>;
        interrupt-parent = <&gpio1>;
        interrupts = <RK_PC6 IRQ_TYPE_EDGE_RISING>;
        interrupt-names = "wlan-host-wake";
        pinctrl-names = "default";
        pinctrl-0 = <&wlan_host_wake_irq>;
    };
};

/* I2S -------------------------------------------------------------- */
&i2s0 {
    #sound-dai-cells = <0>;
    status = "okay";

    pinctrl-names = "default";
    pinctrl-0 = <&i2s0_8ch_bus>;

    rockchip,playback-channels = <2>;
    rockchip,capture-channels = <2>;
};

&i2s1 {
    #sound-dai-cells = <0>;
    status = "okay";
};

&i2s2 {
    #sound-dai-cells = <0>;
    status = "okay";
};

/* ------------------------------------------------------------------ */
&display_subsystem {
    status = "okay";

    route {
        route_hdmi: route-hdmi {
            connect = <&vopb_out_hdmi>;
        };

        route_mipi: route-mipi {
            connect = <&vopb_out_mipi>;
        };
    };
};

&mipi_dsi {
    status = "okay";

    ports {
        mipi_out: port@1 {
            reg = <1>;
            #address-cells = <1>;
            #size-cells = <0>;

            mipi_host_out: endpoint {
                remote-endpoint = <&bridge_mipi_in>;
            };
        };
    };

    bridge {
        compatible ="ti,sn65dsi83";
        reg = <0>;

        dsi,flags = <(MIPI_DSI_MODE_VIDEO | MIPI_DSI_MODE_VIDEO_BURST |
                      MIPI_DSI_MODE_VIDEO_SYNC_PULSE)>;
        dsi,format = <MIPI_DSI_FMT_RGB888>;
        dsi,lanes = <4>;

        i2c,client = <&sn65dsi83_i2c>;

        ports {
            #address-cells = <1>;
            #size-cells = <0>;

            bridge_in: port@0 {
                reg = <0>;
                #address-cells = <1>;
                #size-cells = <0>;

                bridge_mipi_in: endpoint {
                    remote-endpoint = <&mipi_host_out>;
                };
            };

            bridge_out: port@1 {
                reg = <1>;
                #address-cells = <1>;
                #size-cells = <0>;

                bridge_lvds_out: endpoint {
                    remote-endpoint = <&lvds_panel_in>;
                };
            };
        };
    };
};

/* ------------------------------------------------------------------ */
&mipi_dphy_rx0 {
    status = "okay";
};

&isp0 {
    status = "okay";

    ports {
        port@0 {
            isp0_csi1_ep1: endpoint@1 {
                reg = <1>;
                remote-endpoint = <&adv7280m_out>;
                clock-lanes = <0>;
                data-lanes = <1>;
            };
        };
    };
};
